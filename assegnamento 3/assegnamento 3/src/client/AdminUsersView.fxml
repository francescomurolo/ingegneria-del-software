<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.AdminUsersViewController">
   <children>
      <VBox alignment="TOP_CENTER">
         <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Users">
                    <font>
                        <Font size="38.0" />
                    </font>
            </Text>
            <AnchorPane>
               <children>
                        <Button fx:id="backButton" mnemonicParsing="false" text="Back" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
                <TableView fx:id="userTable" prefHeight="500.0" prefWidth="800.0">
                    <columns>
                        <TableColumn fx:id="nameColumn" maxWidth="280.0" minWidth="280.0" prefWidth="280.0" resizable="false" text="Name" />
                        <TableColumn fx:id="surnameColumn" maxWidth="260.0" minWidth="260.0" prefWidth="260.0" resizable="false" text="Surname" />
                        <TableColumn fx:id="emailColumn" maxWidth="260.0" minWidth="260.0" prefWidth="260.0" resizable="false" text="Email" />
                    </columns>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
