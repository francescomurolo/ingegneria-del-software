<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.EmployeeNewWineViewController">
   <children>
      <VBox alignment="TOP_CENTER">
         <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="New wine">
                      <font>
                          <Font size="38.0" />
                      </font>
                  </Text>
                <HBox>
                    <children>
                  <AnchorPane prefWidth="400.0">
                     <children>
                              <Button fx:id="backButton" mnemonicParsing="false" text="Back" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane prefWidth="400.0">
                     <children>
                              <Button fx:id="createButton" mnemonicParsing="false" text="Create" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                    </children>
                </HBox>
                <VBox>
                    <children>
                        <TextField fx:id="nameTextField" promptText="Name">
                            <VBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                            <font>
                                <Font size="22.0" />
                            </font>
                  </TextField>
                        <TextField fx:id="producerTextField" promptText="Producer">
                            <VBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                            <font>
                                <Font size="22.0" />
                            </font>
                  </TextField>
                  <TextField fx:id="yearTextField" promptText="Year">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <font>
                        <Font size="22.0" />
                     </font>
                  </TextField>
                        <TextField fx:id="notesTextField" promptText="Notes">
                            <VBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                            <font>
                                <Font size="22.0" />
                            </font>
                  </TextField>
                        <TextField fx:id="vinesTextField" promptText="Vines">
                            <VBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                            <font>
                                <Font size="22.0" />
                            </font>
                  </TextField>
                  <TextField fx:id="quantityTextField" promptText="Quantity">
                     <font>
                        <Font size="22.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </TextField>
                    </children>
               <VBox.margin>
                  <Insets left="100.0" right="100.0" top="20.0" />
               </VBox.margin>
                </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
