<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.EmployeeOrdersViewController">
   <children>
      <VBox alignment="TOP_CENTER">
         <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Orders">
                    <font>
                        <Font size="38.0" />
                    </font>
            </Text>
                <HBox alignment="TOP_CENTER">
                    <children>
                  <AnchorPane prefWidth="400.0">
                     <children>
                              <Button fx:id="backButton" mnemonicParsing="false" text="Back" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane prefWidth="400.0">
                     <children>
                        <Button fx:id="shipButton" disable="true" mnemonicParsing="false" style="-fx-background-color: lightgreen;" text="Ship" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                    </children>
                </HBox>
                <TableView fx:id="orderTable" prefHeight="500.0" prefWidth="800.0">
                    <columns>
                        <TableColumn fx:id="wineColumn" maxWidth="320.0" minWidth="320.0" prefWidth="320.0" resizable="false" text="Wine" />
                        <TableColumn fx:id="userColumn" maxWidth="320.0" minWidth="320.0" prefWidth="320.0" resizable="false" text="client" />
                        <TableColumn fx:id="quantityColumn" maxWidth="70.0" minWidth="70.0" prefWidth="70.0" resizable="false" text="Quantity" />
                        <TableColumn fx:id="statusColumn" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" resizable="false" text="Status" />
                    </columns>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
